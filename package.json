  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@zoomus/websdk": "^1.7.9",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jquery": "^3.5.1",
    "mongoose": "^5.9.25",
    "react-bootstrap": "^1.2.2"
  }
}
